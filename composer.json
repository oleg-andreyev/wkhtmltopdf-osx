{
    "name": "oleg-andreyev/wkhtmltopdf-osx",
    "description": "Convert html to pdf using webkit (qtwebkit). Static linked binary for MacOS systems.",
    "keywords": [
        "pdf",
        "thumbnail",
        "snapshot",
        "wkhtmltopdf",
        "convert",
        "binary"
    ],
    "license": "LGPL-3.0-only",
    "homepage": "http://wkhtmltopdf.org/",
    "authors": [
        {
            "name": "Oleg Andreyev",
            "email": "oleg@andreyev.lv"
        }
    ],
    "minimum-stability": "stable",
    "bin": [
        "bin/wkhtmltopdf-amd64",
        "bin/wkhtmltopdf-i386"
    ],
    "autoload": {
        "psr-4": {
            "OAndreyev\\WKHTMLToPDF\\": ""
        }
    },
    "require-dev": {
        "knplabs/knp-snappy": "^1.0"
    }
}
